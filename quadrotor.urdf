<?xml version="1.0"?>
<robot name="quadrotor">

  <!-- Main body -->
  <link name="base_link">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.5"/>
      <inertia ixx="0.0023" ixy="0.0" ixz="0.0"
               iyy="0.0023" iyz="0.0" izz="0.004"/>
    </inertial>
    <visual>
      <geometry>
        <box size="0.2 0.2 0.05"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <box size="0.2 0.2 0.05"/>
      </geometry>
    </collision>
  </link>

  <!-- Rotor 1 -->
  <link name="rotor1">
    <inertial>
      <origin xyz="0 0 0"/>
      <mass value="0.01"/>
      <inertia ixx="1e-5" iyy="1e-5" izz="1e-5"/>
    </inertial>
    <visual>
      <geometry>
        <cylinder radius="0.05" length="0.01"/>
      </geometry>
    </visual>
  </link>
  <joint name="rotor1_joint" type="continuous">
    <parent link="base_link"/>
    <child link="rotor1"/>
    <origin xyz="0.15 0 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
  </joint>

  <!-- Rotor 2 -->
  <link name="rotor2">
    <inertial>
      <origin xyz="0 0 0"/>
      <mass value="0.01"/>
      <inertia ixx="1e-5" iyy="1e-5" izz="1e-5"/>
    </inertial>
    <visual>
      <geometry>
        <cylinder radius="0.05" length="0.01"/>
      </geometry>
    </visual>
  </link>
  <joint name="rotor2_joint" type="continuous">
    <parent link="base_link"/>
    <child link="rotor2"/>
    <origin xyz="0 0.15 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
  </joint>

  <!-- Rotor 3 -->
  <link name="rotor3">
    <inertial>
      <origin xyz="0 0 0"/>
      <mass value="0.01"/>
      <inertia ixx="1e-5" iyy="1e-5" izz="1e-5"/>
    </inertial>
    <visual>
      <geometry>
        <cylinder radius="0.05" length="0.01"/>
      </geometry>
    </visual>
  </link>
  <joint name="rotor3_joint" type="continuous">
    <parent link="base_link"/>
    <child link="rotor3"/>
    <origin xyz="-0.15 0 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
  </joint>

  <!-- Rotor 4 -->
  <link name="rotor4">
    <inertial>
      <origin xyz="0 0 0"/>
      <mass value="0.01"/>
      <inertia ixx="1e-5" iyy="1e-5" izz="1e-5"/>
    </inertial>
    <visual>
      <geometry>
        <cylinder radius="0.05" length="0.01"/>
      </geometry>
    </visual>
  </link>
  <joint name="rotor4_joint" type="continuous">
    <parent link="base_link"/>
    <child link="rotor4"/>
    <origin xyz="0 -0.15 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
  </joint>

</robot>
